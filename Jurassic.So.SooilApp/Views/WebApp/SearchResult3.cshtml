    //点击空白隐藏
    $(function () {
        $(document).click(function (e) {
            if (e.target.className == "yxj_eng") {
                return false;
            } else {
                $(".select_option").hide();
            }
        });
        $("#choose").val("0");

        $.ajax({

            type: "POST",
            crossDomain: true,
            url: "@urlPrefix/words/getTop5HotWord",
            success: function(data) {
                for(var i = 0; i < data.length; i++){
                    var result = "<a href='/search?t=0&p=1&w="+data[i].word+"' title='"+data[i].word+"'>"+data[i].word+"</a>";
                    $("#hotword").append(result);
                }
            }
        });
    })

    function handle(){
        var txtValue = $("#searchContentId").val().replace("'","");

        $.ajax({
            type: "POST",
            url: "/words/wordsearch",
            data: {"word": txtValue},
            success: function(data) {
                $("#enlist").html("");//刷新下拉列表
                if(data.length > 0){
                    entryList = data;
                    for(var i = 0; i < data.length; i++){
                        var result = "<li><a href='/search?t=0&p=1&w="+data[i].word+"' title='"+data[i].word+"'><p>"+subString(data[i].word, 40, true)+"</p></a></li>";
                        $("#enlist").append(result);
                    }
                    $(".yxj_tc li").mouseover(function(e) {
                        $(this).addClass("on").siblings().removeClass("on");
                    });
                }
                $(".yxj_tc").show();
            }
        });
        //放大镜图标隐藏
//        if(txtValue==''){
//            $('#icon-search').show();
//        }else{
//            $('#icon-search').hide();
//        }
    }
    //点击搜索框时隐藏放大镜图标
    $(function () {
        $('#searchContentId').focus(function () {
            $('#icon-search').hide();
        })
        $('#searchContentId').blur(function () {
            $('#icon-search').show();
        })
    });
    //------------下拉框键盘上下移动效果-----------------

    $(document).keydown(function(e) {
        var len=$(".yxj_tc li").length;
        if(event.keyCode==40){
            var li=$(".yxj_tc .on");
            var num=li.index();
            if(num==-1){
                $(".yxj_tc li").first().addClass("on");
                $(".yxj_text").val($(".yxj_tc li").first().text());
            }
            if(num==len-1){
                return false;
            }
            else{
                li.removeClass("on");
                li.next().addClass("on");
                var text=$(".yxj_tc .on a").text();
                $(".yxj_text").val(text);
            }
        }
        if(event.keyCode==38){
            var li=$(".yxj_tc .on");
            var num=li.index();
            if(num<1){
                return false;
            }
            else{
                li.removeClass("on");
                li.prev().addClass("on");
                var text=$(".yxj_tc .on a").text();
                $(".yxj_text").val(text);
            }
        }
    });

    //计算字符串长度

    //截取字符串 包含中文处理
    //(串,长度,增加...)
    function subString(str, len, hasDot)
    {
        var newLength = 0;
        var newStr = "";
        var chineseRegex = /[^\x00-\xff]/g;
        var singleChar = "";
        var strLength = str.replace(chineseRegex,"**").length;
        for(var i = 0;i < strLength;i++)
        {
            singleChar = str.charAt(i).toString();
            if(singleChar.match(chineseRegex) != null)
            {
                newLength += 2;
            }
            else
            {
                newLength++;
            }
            if(newLength > len)
            {
                break;
            }
            newStr += singleChar;
        }

        if(hasDot && strLength > len)
        {
            newStr += "...";
        }
        return newStr;
    }

    $(document).click(function (e) {
        $("#enlist").html("");
    });



</script>
    
